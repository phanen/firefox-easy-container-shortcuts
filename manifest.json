{
  "manifest_version": 2,
  "name": "Easy Container Shortcuts",
  "version": "1.6.0",
  "description": "Toggle container",
  "applications": {
    "gecko": {
      "id": "firefox.container-shortcuts@strategery.io"
    }
  },
  "icons": {
    "48": "assets/icon.svg"
  },
  "permissions": [
    "tabs",
    "contextualIdentities",
    "cookies"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "ecs-toggle-container": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Toggle alternative"
    }
  }
}
